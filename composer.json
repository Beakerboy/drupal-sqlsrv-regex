{
  "name": "beakerboy/drupal-sqlsrv-regex",
  "description": "Regular Expression CLR for Sql Server",
  "type": "drupal-module",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Kevin Nowaczyk",
      "role": "Lead Developer"
    }
  ],
  "autoload": {
    "psr-4": { "Drupal\\Driver\\Database\\sqlsrv\\": "drivers/lib/Drupal/Driver/Database/sqlsrv" }
  },
  "autoload-dev": {
    "psr-4": { "Drupal\\Tests\\sqlsrv\\": "tests/src/" }
  },
  "require": {
    "php" : "^7.0",
    "drupal/core" : "^8.6"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "phpstan/phpstan": "*",
    "php-coveralls/php-coveralls": "*"
  }
}
